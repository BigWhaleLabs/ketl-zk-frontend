{
  "name": "ketl-zk-frontend",
  "version": "1.0.0",
  "license": "MIT",
  "prettier": "@big-whale-labs/prettier-config",
  "scripts": {
    "start": "yarn generate-css-types && vite",
    "build": "yarn generate-css-types && cross-env NODE_ENV=production tsc && vite build",
    "pretty": "prettier --check src",
    "lint": "yarn pretty && eslint --max-warnings 0 ./src/",
    "generate-css-types": "tailwindcss-classnames -o src/classnames/tailwind.ts && prettier --write src/classnames/tailwind.ts",
    "preview": "yarn build && yarn vite preview"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@big-whale-labs/constants": "^0.2.8",
    "@big-whale-labs/ketl-attestation-token": "0.1.0-0",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@rollup/plugin-inject": "^5.0.3",
    "@vitejs/plugin-react": "^3.1.0",
    "@zk-kit/incremental-merkle-tree": "^1.0.0",
    "assert-browserify": "^2.0.0",
    "circomlibjs": "^0.1.7",
    "crypto-browserify": "^3.12.0",
    "envalid": "^7.3.1",
    "ffjavascript": "^0.2.56",
    "preact": "^10.12.1",
    "react-textarea-autosize": "^8.4.0",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "valtio": "^1.10.2",
    "vite-plugin-comlink": "^3.0.5",
    "vite-plugin-remove-console": "^2.0.0",
    "wouter": "^2.10.0"
  },
  "devDependencies": {
    "@big-whale-labs/eslint-config-bwl": "^2.1.5",
    "@big-whale-labs/prettier-config": "^1.1.3",
    "@preact/preset-vite": "^2.5.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.13.0",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "eslint": "~8.7.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "react-scripts": "^5.0.1",
    "rollup-plugin-visualizer": "^5.9.0",
    "tailwindcss": "^3.2.6",
    "tailwindcss-classnames": "^3.0.7",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-tsconfig-paths": "^4.0.5"
  },
  "packageManager": "yarn@3.4.1"
}
